{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "paths": {
            "@entities/*": ["src/entities/*"]
        }
    },
    "exclude": [
        "vitest.config.ts"
    ]
}
